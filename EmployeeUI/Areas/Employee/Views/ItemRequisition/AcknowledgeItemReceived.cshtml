
@{
    ViewBag.Title = "AcknowledgeItemReceived";
    Layout = "~/Areas/Employee/Views/Shared/MasterLayout.cshtml";
}

<section class="content-header">
    <h1>Update Requisition</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>General</a></li>
        <li class="breadcrumb-item"><a>Requisition</a></li>
        <li class="breadcrumb-item active"><a>Update Requisition</a></li>
    </ol>
</section>
<br />
<div class="card-footer">
    <button type="submit" class="btn btn-primary" onclick="window.location.href='/Employee/ItemRequisition/ItemRequisitionListPerLoginUser'">My Requisitions</button>
</div>

<section class="content">
    <div class="row">
        <div class="col-md-12">
            @Html.Partial("~/Areas/Employee/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @Html.Partial("Partial/AcknowledgeForm")
                <hr/>
                @using (Html.BeginForm("AcknowledgeItemReceived", "ItemRequisition", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/AcknowledgeSubmitForm")
                }
            </div>
        </div>
    </div>
</section>