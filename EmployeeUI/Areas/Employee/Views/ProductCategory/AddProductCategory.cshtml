
@{
    ViewBag.Title = "AddProductCategory";
    Layout = "~/Areas/Employee/Views/Shared/MasterLayout.cshtml";
}

<section class="content-header">
    <h1>Add Product Category</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Procurement Management</a></li>
        <li class="breadcrumb-item"><a>Products</a></li>
        <li class="breadcrumb-item active"><a>Add Product Category</a></li>
    </ol>

</section>
<br />
<div class="card-footer">
    <button type="submit" class="btn btn-primary" onclick="window.location.href='/Employee/ProductCategory/ProductCategoryList'">Product Category List</button>
</div>

<section class="content">
    <div class="row">
        <div class="col-md-6">
            @Html.Partial("~/Areas/Employee/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("AddProductCategory", "ProductCategory", FormMethod.Post, new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/ProductCategoryForm")
                }
            </div>
        </div>
    </div>
</section>
