
@{
    ViewBag.Title = "FleetVehicleBookingReportDetail";
    Layout = "~/Areas/Employee/Views/Shared/MasterLayout.cshtml";
}


<section class="content-header">
    <h1>View Vehicle Booking Report Detail</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Fleet Management</a></li>
        <li class="breadcrumb-item"><a>Vehicle</a></li>
        <li class="breadcrumb-item active"><a>Vehicle Booking Report</a></li>
    </ol>

</section>
<br />
<div class="card-footer">
    <button type="submit" class="btn btn-primary" onclick="window.location.href='/Employee/FleetVehicleBookingReport/AddFleetVehicleBookingReport'">Add New Vehicle Booking Report</button>
</div>

<section class="content">
    <div class="row">
        <div class="col-md-12">
            @Html.Partial("~/Areas/Employee/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("FleetVehicleBookingReportDetail", "FleetVehicleBookingReport", FormMethod.Get, new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/FleetVehicleBookingReportDetails")
                }
            </div>
        </div>
    </div>
</section>
