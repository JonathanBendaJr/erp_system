
@{
    ViewBag.Title = "FleetBookingRequestDetails";
    Layout = "~/Areas/Employee/Views/Shared/MasterLayout.cshtml";
}

<section class="content-header">
    <h1>View Vehicle Booking Request</h1>
    <ol class="breadcrumb float-sm-left">
        <li class="breadcrumb-item"><a>Fleet Management</a></li>
        <li class="breadcrumb-item"><a>Vehicle</a></li>
        <li class="breadcrumb-item active"><a>View Vehicle Booking Request</a></li>
    </ol>

</section>
<br />
<div class="card-footer">
    <button type="submit" class="btn btn-primary" onclick="window.location.href='/Employee/FleetVehicleBooking/FleetVehicleBookingList'">Vehicle Booking List</button>
</div>

<section class="content">
    <div class="row">
        <div class="col-md-6">
            @Html.Partial("~/Areas/Employee/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("AddFleetVehicleBooking", "FleetVehicleBooking", new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/FleetVehicleBookingDetails")
                }
            </div>
        </div>
        <div class="col-md-6">
            @Html.Partial("~/Areas/Employee/Views/Shared/MessageForm.cshtml")
            <div class="box box-primary">
                @using (Html.BeginForm("AddFleetVehicleBooking", "FleetVehicleBooking", FormMethod.Get, new { @enctype = "multipart/form-data" }))
                {
                    @Html.Partial("Partial/DeptManagerApprovalDetails")
                }
            </div>
        </div>
    </div>
</section>
